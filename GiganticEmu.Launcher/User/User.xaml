<rxui:ReactiveUserControl x:Class="GiganticEmu.Launcher.User"
    x:TypeArguments="local:UserViewModel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:rxui="http://reactiveui.net"
    xmlns:local="clr-namespace:GiganticEmu.Launcher" mc:Ignorable="d" d:DesignHeight="450" d:DesignWidth="600">

    <DockPanel KeyboardNavigation.TabNavigation="Cycle">
        <Button x:Name="ButtonLogout" TabIndex="2" DockPanel.Dock="Bottom" Margin="0,-5,0,0" Style="{StaticResource MaterialDesignFlatButton}" Content="Log out" HorizontalAlignment="Center" />
        <Button x:Name="ButtonStartGame" TabIndex="1" DockPanel.Dock="Bottom" Content="Start Game" FontSize="32" Margin="0 20" Height="64" />
        <Canvas></Canvas>
    </DockPanel>
</rxui:ReactiveUserControl>
